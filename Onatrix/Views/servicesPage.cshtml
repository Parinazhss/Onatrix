@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = "mainLayout.cshtml";

    var backgroundImgRight = Model?.Value<IPublishedContent>("backgroundImgRight")?.Url() ?? "";

    var backgroundImgLeft = Model?.Value<IPublishedContent>("backgroundImgLeft")?.Url() ?? "";



	
}
<section class="atf">
            <img src="@backgroundImgRight" class="earth-down">
            <img src="@backgroundImgLeft" class="earth-up">
      
          <div class="atf-content">
            <h1>@Model?.Value("pageTitle")</h1>
            <nav class="breadcrumb-nav">
              <a href="/">@Model?.Value("textHome")</a>
               <i class="fa-solid fa-angle-right"></i>
              <a href="/services">@Model?.Value("textServices")</a>
            </nav>
          </div>
 </section>
